{
    "name": "yukata-roommate/simple-logger",
    "description": "Simple Logger for PHP",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "YukataRm\\SimpleLogger\\": "src",
            "YukataRm\\Laravel\\SimpleLogger\\": "src\\Laravel"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YukataRm\\Laravel\\SimpleLogger\\Provider\\ServiceProvider"
            ],
            "aliases": {
                "Logger": "YukataRm\\Laravel\\SimpleLogger\\Facade\\Logger"
            }
        }
    },
    "authors": [
        {
            "name": "Yukata Roommate",
            "email": "contact@yukata-roommate.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "yukata-roommate/static-proxy": "^1.0.0",
        "yukata-roommate/env-loader": "^1.0.0"
    },
    "require-dev": {
        "laravel/laravel": "^11.0"
    },
    "minimum-stability": "dev"
}